# Database Configuration
# For local development, you can use SQLite
DATABASE_URL="file:local.db"
# For Turso database (if used)
# DATABASE_URL="libsql://your-database.turso.io"
DATABASE_AUTH_TOKEN="your-auth-token"

# AI Model API Keys
# Get your API key from Google AI Studio
Gemini_API_KEY="your-gemini-api-key"
Gemini_Model="gemini-1.5-flash"

# Get your API key from Mistral AI
MISTRAL_API_KEY="your-mistral-api-key"

# Database Credentials (if using Supabase or similar)
Supabase_DB_Password="your-db-password"
DATABASE_URL="postgresql://postgres.your-project.supabase.co:6543/postgres?pgbouncer=true"
DIRECT_URL="postgresql://postgres.your-project.supabase.co:5432/postgres"

# Admin Password (hashed)
Admin_PW="your-hashed-admin-password"

# Error Tracking
PUBLIC_SENTRY_DSN="your-sentry-dsn"

# GitHub Configuration
VITE_ACCESS_TOKEN="your-github-pat"
VITE_GITHUB_TOKEN="your-github-token"

# Authentication Configuration
AUTH_GITHUB_ID="your-github-oauth-id"
AUTH_GITHUB_SECRET="your-github-oauth-secret"

AUTH_GOOGLE_ID="your-google-oauth-id"
AUTH_GOOGLE_SECRET="your-google-oauth-secret"

# Authentication Secrets
AUTH_SECRET="your-auth-secret"
JWT_SECRET="your-jwt-secret"

# Exchange Rate API Configuration
# Get your API key from https://www.exchangerate-api.com
EXCHANGE_RATE_API_KEY="your-exchange-rate-api-key"
